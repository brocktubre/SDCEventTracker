<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<!-- saved from url=(0055)http://www.sqdog.com/SDC_Guidelines/SDC_Guidelines.html -->
<html>
<!-- #BeginTemplate "../../SqDog.data/Templates/Blank_No_Left_Nav.html" -->
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css    ")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/jquery-ui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/styles.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.datepicker.css")" rel="stylesheet" />
    @RenderSection("Styles", false)
    <script src="@Url.Content("~/Content/js/jquery-1.10.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery-ui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/scripts.js")" type="text/javascript"></script>
    @RenderSection("JavaScript", required: false)

</head>

<body background="@Url.Content("~/Content/Welcome to Squirrel Dog Central_files/pagebackground.jpg")" leftmargin="0" marginheight="0" marginwidth="0" topmargin="0">
    <table width="100%" border="0" cellspacing="10" cellpadding="0" background="@Url.Content("~/Content/Welcome to Squirrel Dog Central_files/hickory.jpg")" height="150">
        <tbody>
            <tr background="none">
                <td width="20%">
                    <div align="center">
                        <a href="http://www.sqdog.com/"><img src="@Url.Content("~/Content/Welcome to Squirrel Dog Central_files/logo.jpg")" alt="" height="125" width="166" border="0"></a>
                    </div>
                </td>
                <td background="@Url.Content("~/Content/Welcome to Squirrel Dog Central_files/hardwoodfloor.jpg")">
                    <div align="center">
                        <table border="0" cellspacing="15" cellpadding="0" background="@Url.Content("~/Content/Welcome to Squirrel Dog Central_files/hickory.jpg")">
                            <tbody>
                                <tr>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
    <!-- #BeginEditable "Region2" -->
    <table width="100%" border="0" cellspacing="7" cellpadding="0" background="@Url.Content("~/Content/Welcome to Squirrel Dog Central_files/hickory.jpg")">
        <tbody>
            <tr>
                <td background="@Url.Content("~/Content/Welcome to Squirrel Dog Central_files/pagebackground.jpg")">
                    <br />
                </td>
            </tr>
        </tbody>
    </table>

    <!-- Small button group -->
    <div class="navbar navbar-inverse navbar-fixed-top" style="height: 0;">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("SDC Event Tracker", "Competitions", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Competitions", "Competitions", "Home")</li>
                    <li>@Html.ActionLink("Post Upcoming Hunt", "SubmitEvent", "Home")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody() <!-- Renders body content "where the magic happens"-->
        <footer style="padding-top: 100px;">
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryui")
    @RenderSection("scripts", required: false)

    <hr size="9">
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tbody>
            <tr>
                <td background="@Url.Content("~/Content/Welcome to Squirrel Dog Central_files/pagebackground.jpg")">
                    <div align="center">
                        <p></p>
                        <p>Copyright &copy; Sqdog</p>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
    <p></p>
    <!-- #EndTemplate -->
</body>
</html>

